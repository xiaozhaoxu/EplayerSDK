<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="chat_add_view_style">
        <item name="android:layout_width">0.5dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_marginTop">@dimen/height_12</item>
        <item name="android:layout_marginBottom">@dimen/height_12</item>
        <item name="android:background">@color/common_light_text_color</item>
    </style>

    <style name="social_bt_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/button_bg_style</item>
        <item name="android:textColor">@drawable/button_font_style</item>
        <item name="android:textSize">@dimen/title3_textsize</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="MediaController_SeekBar" parent="android:Widget.SeekBar">
        <item name="android:progressDrawable">@drawable/scrubber_progress_horizontal_holo_dark</item>
        <item name="android:indeterminateDrawable">@drawable/scrubber_progress_horizontal_holo_dark</item>
        <item name="android:minHeight">1dip</item>
        <item name="android:maxHeight">1dip</item>
        <item name="android:thumb">@drawable/scrubber_control_selector_holo</item>
        <item name="android:thumbOffset">@dimen/phone_seekbar_left_padding</item>
        <item name="android:paddingLeft">@dimen/phone_seekbar_left_padding</item>
        <item name="android:paddingRight">@dimen/phone_seekbar_left_padding</item>
    </style>


</resources>